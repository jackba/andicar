<!-- spinner initialization arrays-->
<!--
    !!!For translators!!!:
    Only some items must be translated. For example the items with the "Do not use" text
-->
<resources>
    <!-- report file formats  -->
    <string-array name="report_formats">
        <item>csv</item>
        <item>html</item>
    </string-array>

    <!-- Used in gps track (minimum time betwee 2 recording points) -->
    <string-array name="gpstrack_preference_mintime_entries">
        <item>最高</item>
        <item>3 秒</item>
        <item>5 秒</item>
        <item>10 秒</item>
        <item>30 秒</item>
        <item>1 分钟</item>
        <item>3 分钟</item>
        <item>5 分钟</item>
        <item>10 分钟</item>
    </string-array>
    <!-- values in miliseconds -->
    <string-array name="gpstrack_preference_mintime_values">
        <item>0</item>
        <item>3000</item>
        <item>5000</item>
        <item>10000</item>
        <item>30000</item>
        <item>60000</item>
        <item>180000</item>
        <item>300000</item>
        <item>600000</item>
    </string-array>

    <!-- Used in gps track (maximum value of accuracy) -->
    <string-array name="gpstrack_preference_maxaccuracy_entries">
        <item>10 分钟 / 10.94 码</item>
        <item>20 分钟 / 21.87 码</item>
        <item>50 分钟 / 54.68 码</item>
        <item>100 分钟 / 109.36 码</item>
        <item>250 分钟 / 273.40 码</item>
        <item>500 分钟 / 546.80 码</item>
        <item>"不使用"</item>
    </string-array>
    <!-- values in meter -->
    <string-array name="gpstrack_preference_maxaccuracy_values">
        <item>10</item>
        <item>20</item>
        <item>50</item>
        <item>100</item>
        <item>250</item>
        <item>500</item>
        <item>9999999</item>
    </string-array>

    <!-- Used in gps track. Maximum value of percentage of skipped track points.
    Tracking will be automatically stopped over this limit -->
    <string-array name="gpstrack_preference_maxaccuracyshutdownlimit_entries">
        <item>10%</item>
        <item>20%</item>
        <item>30%</item>
        <item>50%</item>
        <item>70%</item>
        <item>"不使用"</item>
    </string-array>
    <!-- values in meter -->
    <string-array name="gpstrack_preference_maxaccuracyshutdownlimit_values">
        <item>10</item>
        <item>20</item>
        <item>30</item>
        <item>50</item>
        <item>70</item>
        <item>999</item>
    </string-array>

    <string-array name="uom_type_entries">
        <item>长度</item>
        <item>容量</item>
        <item>其它</item>
    </string-array>

    <string-array name="ao_bk_schedule_type_entries">
		<item>每天</item>
		<item>每周</item>
    </string-array>
</resources>
